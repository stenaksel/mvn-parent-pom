            </configuration>
          </execution>
          <execution>
            <id>mvn-parent-pom maven-enforcer-plugin requirePluginVersions</id>
            <goals>
              <goal>enforce</goal>
            </goals>
            <configuration>
              <rules>
                <requirePluginVersions>
                  <!-- // @formatter:off -->
                  <message>${i18n.requirePluginVersions}</message>
                  <!-- // @formatter:on -->
                  <banLatest>true</banLatest>
                  <banRelease>true</banRelease>
                  <banSnapshots>true</banSnapshots>
                  <phases>clean,deploy,site</phases>
                  <additionalPlugins>
                    <additionalPlugin>org.apache.maven.plugins:maven-reactor-plugin</additionalPlugin>
                  </additionalPlugins>
                  <unCheckedPluginList>org.apache.maven.plugins:maven-enforcer-plugin</unCheckedPluginList>
                </requirePluginVersions>
              </rules>
            </configuration>
          </execution>
          <execution>
            <id>mvn-parent-pom(maven-enforcer-plugin) dependencyConvergence</id>
            <goals>
              <goal>enforce</goal>
            </goals>
            <configuration>
              <rules>
                <dependencyConvergence>
                  <!-- // @formatter:off -->
                  <!--//TODO
                  <message>Maven kjørte "${project.name}" med følgende parametere [sun.java.command]:
      ${sun.java.command}
           _                           _                         ___
        __| | ___ _ __   ___ _ __   __| | ___ _ __   ___ _   _  / __\___  _ ____   _____ _ __ __ _  ___ _ __   ___ ___
       / _` |/ _ \ '_ \ / _ \ '_ \ / _` |/ _ \ '_ \ / __| | | |/ /  / _ \| '_ \ \ / / _ \ '__/ _` |/ _ \ '_ \ / __/ _ \
      | (_| |  __/ |_) |  __/ | | | (_| |  __/ | | | (__| |_| / /__| (_) | | | \ V /  __/ | | (_| |  __/ | | | (_|  __/
       \__,_|\___| .__/ \___|_| |_|\__,_|\___|_| |_|\___|\__, \____/\___/|_| |_|\_/ \___|_|  \__, |\___|_| |_|\___\___|
                 |_|                                     |___/                               |___/
        Alle plugins som benyttes må ha en spesifisert versjon!
      ==============================================================
      Krav konfigurert i mvn-parent-pom ('dependencyConvergence')
                  </message>
-->
                  <!-- // @formatter:on -->
                  <uniqueVersions>true</uniqueVersions>
                </dependencyConvergence>
              </rules>
            </configuration>
          </execution>
          <execution>
            <id>mvn-parent-pom(maven-enforcer-plugin) banDuplicatePomDependencyVersions</id>
            <goals>
              <goal>enforce</goal>
            </goals>
            <configuration>
              <rules>
                <banDuplicatePomDependencyVersions>
                  <message>Duplication of dependencies found - Needs same version !</message>
                </banDuplicatePomDependencyVersions>
              </rules>
              <fail>true</fail>
            </configuration>
          </execution>
          <execution>
            <id>mvn-parent-pom(maven-enforcer-plugin) reactorModuleConvergence</id>
            <goals>
              <goal>enforce</goal>
            </goals>
            <configuration>
              <rules>
                <reactorModuleConvergence>
                  <message>The reactor is not valid</message>
                  <ignoreModuleDependencies>true</ignoreModuleDependencies>
                </reactorModuleConvergence>
              </rules>
              <fail>true</fail>
            </configuration>
          </execution>
        </executions>
      </plugin>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-failsafe-plugin</artifactId>
        <configuration>
          <forkCount>1</forkCount>
          <reuseForks>false</reuseForks>
        </configuration>
        <executions>
          <!-- Ensures that both integration-test and verify goals of the Failsafe Maven plugin are executed. -->
          <execution>
            <id>mvn-parent-pom(maven-failsafe-plugin)</id>
            <goals>
              <goal>integration-test</goal>
              <goal>verify</goal>
            </goals>
          </execution>
        </executions>
      </plugin>
    </plugins>
  </build>
  
