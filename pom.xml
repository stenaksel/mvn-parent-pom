  <profiles>
    <profile>
      <id>About</id>
      <activation>
        <activeByDefault>true</activeByDefault>
        <property>
          <name>!skip-about</name>
          <value>true</value>
        </property>
      </activation>
      <properties>
        <profile-in-use_About>Yes (skip-about=${skip-about},mvn-debug-reduced=${mvn-debug-reduced}))</profile-in-use_About>
        <skip-about>false (Overridden by activated profile: "About")</skip-about>
      </properties>
      <build>
        <plugins>
          <plugin>
            <groupId>com.github.ekryd.echo-maven-plugin</groupId>
            <artifactId>echo-maven-plugin</artifactId>
            <version>${mvn-echo-maven-plugin.version}</version>
            <inherited>true</inherited>
            <executions>
              <execution>
                <id>mvn-parent-pom About</id>
                <goals>
                  <goal>echo</goal>
                </goals>
                <phase>validate</phase>
                <configuration>
                  <!--suppress UnresolvedMavenProperty -->
                  <!-- // @formatter:off -->
                  <message>${line.separator}
        /${mvn-debug-msg-seperator}\
        |                                                                                       |
        |  "${project.name}" About
        |                                                                                       |
        \${mvn-debug-msg-seperator}/
        ${i18n.about}
        ${mvn-debug-msg}</message>
                  <!-- // @formatter:on -->
                </configuration>
              </execution>
            </executions>
          </plugin>
          <plugin>
            <groupId>org.apache.maven.plugins</groupId>
            <artifactId>maven-help-plugin</artifactId>
            <executions>
              <execution>
                <id>mvn-parent-pom About all-profiles</id>
                <goals>
                  <goal>all-profiles</goal>
                </goals>
                <phase>validate</phase>
              </execution>
            </executions>
          </plugin>
        </plugins>
        
      </build>
