          <execution>
            <id>enforce-reactor-OK mvn-parent-pom</id>
            <goals>
              <goal>enforce</goal>
            </goals>
            <configuration>
              <rules>
                <reactorModuleConvergence>
                  <message>The reactor is not valid</message>
                  <ignoreModuleDependencies>true</ignoreModuleDependencies>
                </reactorModuleConvergence>
              </rules>
              <fail>true</fail>
            </configuration>
          </execution>
          <execution>
            <id>enforce4mvn-parent-pom</id>
            <goals>
              <goal>enforce</goal>
            </goals>
            <configuration>
              <rules>
                <!--                <banDuplicatePomDependencyVersions />-->
                <!--
                                <banTransitiveDependencies>
                                  <excludes>
                                    &lt;!&ndash;                    <exclude>commons-lang</exclude>&ndash;&gt;
                                  </excludes>
                                  <includes>
                                    &lt;!&ndash;                    <include>commons-lang:commons-lang:2.4</include>&ndash;&gt;
                                  </includes>
                                </banTransitiveDependencies>
                -->
                <bannedDependencies>
                  <excludes>
                    <exclude>commons-collections:commons-collections:[3.0,3.2.1]</exclude>
                    <exclude>commons-collections:commons-collections:4.0</exclude>
                    <exclude>org.codehaus.jackson:jackson-mapper-asl:1.9.13</exclude>
                  </excludes>
                </bannedDependencies>
              </rules>
            </configuration>
          </execution>
        </executions>
      </plugin>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-failsafe-plugin</artifactId>
        <version>${maven-failsafe-plugin.version}</version>
        <configuration>
          <forkCount>1</forkCount>
          <reuseForks>false</reuseForks>
        </configuration>
        <executions>
          <!-- Ensures that both integration-test and verify goals of the Failsafe Maven plugin are executed. -->
          <execution>
            <goals>
              <goal>integration-test</goal>
              <goal>verify</goal>
            </goals>
          </execution>
        </executions>
      </plugin>
    </plugins>
  </build>

  <distributionManagement>
    <repository>
      <id>releases</id>
      <url>http://aurora/nexus/content/repositories/releases</url>
    </repository>
    <snapshotRepository>
      <id>snapshots</id>
      <name>Internal Snapshots</name>
      <url>http://aurora/nexus/content/repositories/snapshots</url>
    </snapshotRepository>
  </distributionManagement>

  <profiles>
    <profile>
      <id>mvn-debug</id>
      <activation>
        <activeByDefault>false</activeByDefault>
        <property>
          <name>!skip-mvn-debug</name>
          <value>true</value>
        </property>
      </activation>
      <properties>
        <profile-in-use-info>Profile "mvn-debug" is in use!</profile-in-use-info>
        <profile-in-use_mvn-debug>Yes (skip-mvn-debug=${skip-mvn-debug})(skip-mvn_debug=${skip-mvn_debug})
        </profile-in-use_mvn-debug>
        <show-version-info>true</show-version-info>
        <skip-mvn-debug>(Overridden by profile activation: "mvn-debug")</skip-mvn-debug>
      </properties>
      <build>
        <plugins>
          <plugin>
            <groupId>com.github.ekryd.echo-maven-plugin</groupId>
            <artifactId>echo-maven-plugin</artifactId>
            <version>${mvn-echo-maven-plugin.version}</version>
            <inherited>true</inherited>
            <executions>
              <execution>
                <id>show-message_none</id>
                <goals>
                  <goal>echo</goal>
                </goals>
                <phase>none</phase>
                <configuration>
                  <message><![CDATA[${line.separator}
              skip-mvn-debug=${skip-mvn-debug}
              skip-mvn_debug=${skip-mvn_debug}
                =======================================================================================
                n o n e
                =======================================================================================]]></message>
                </configuration>
              </execution>
              <execution>
                <id>show-message_validate</id>
                <goals>
                  <goal>echo</goal>
                </goals>
                <phase>validate</phase>
                <configuration>
                  <!-- Text for Maven phases created with: http://patorjk.com/software/taag/#p=display&f=Ogre -->
                  <message><![CDATA[${line.separator}
              skip-mvn-debug=${skip-mvn-debug}
              skip-mvn_debug=${skip-mvn_debug}
                =======================================================================================
                                 _   _       _           _
                __   __   __ _  | | (_)   __| |   __ _  | |_    ___
                \ \ / /  / _` | | | | |  / _` |  / _` | | __|  / _ \
                 \ V /  | (_| | | | | | | (_| | | (_| | | |_  |  __/
                  \_/    \__,_| |_| |_|  \__,_|  \__,_|  \__|  \___|
                =======================================================================================]]></message>
                </configuration>
              </execution>
              <execution>
                <id>show-message_clean</id>
                <goals>
                  <goal>echo</goal>
                </goals>
                <phase>clean</phase>
                <configuration>
                  <!--suppress UnresolvedMavenProperty -->
                  <message><![CDATA[${line.separator}
              skip-mvn-debug=${skip-mvn-debug}
              skip-mvn_debug=${skip-mvn_debug}
                =======================================================================================
                        _
                  ___  | |   ___    __ _   _ __
                 / __| | |  / _ \  / _` | | '_ \
                | (__  | | |  __/ | (_| | | | | |
                 \___| |_|  \___|  \__,_| |_| |_|

                =======================================================================================
                Maven kjørte "${project.build.finalName}" (${basedir})
                (${build.timestamp})
                (${session.executionRootDirectory})
                ---------------------------------------------------------------------------------------
                ${profile-in-use-info} (Reported by mvn-debug)
                Activate profiles by default by adding them to your ~/.m2/settings.xml file,
                like shown below:
                <activeProfiles>
                  <activeProfile>nexus</activeProfile>
                  <activeProfile>POM-sort</activeProfile>
                  <activeProfile>mvn-debug</activeProfile>
                </activeProfiles>

                Activate the "mvn-debug" profile to get hints for the use and configuration of your project.
                Activate the "POM-sort" profile to keep the POM file sorted.
                =======================================================================================
                | Det er mvn-parent-pom som har konfigurert deler av dette bygget,
                | "${project.build.finalName}" kjøres av Maven (v ${maven.version})
                | (${maven.home})
                =======================================================================================]]></message>
                </configuration>
              </execution>
              <execution>
                <id>show-message_compile</id>
                <goals>
                  <goal>echo</goal>
                </goals>
                <phase>compile</phase>
                <configuration>
                  <message><![CDATA[${line.separator}
              skip-mvn-debug=${skip-mvn-debug}
              skip-mvn_debug=${skip-mvn_debug}
                =======================================================================================
                                                    _   _
                  ___    ___    _ __ ___    _ __   (_) | |   ___
                 / __|  / _ \  | '_ ` _ \  | '_ \  | | | |  / _ \
                | (__  | (_) | | | | | | | | |_) | | | | | |  __/
                 \___|  \___/  |_| |_| |_| | .__/  |_| |_|  \___|
                                           |_|
                =======================================================================================]]></message>
                </configuration>
              </execution>
              <execution>
                <id>show-message_test</id>
                <goals>
                  <goal>echo</goal>
                </goals>
                <phase>test</phase>
                <configuration>
                  <message><![CDATA[${line.separator}
              skip-mvn-debug=${skip-mvn-debug}
              skip-mvn_debug=${skip-mvn_debug}
                =======================================================================================
                 _                  _
                | |_    ___   ___  | |_
                | __|  / _ \ / __| | __|
                | |_  |  __/ \__ \ | |_
                 \__|  \___| |___/  \__|
                =======================================================================================]]></message>
                </configuration>
              </execution>
              <execution>
                <id>show-message_package</id>
                <goals>
                  <goal>echo</goal>
                </goals>
                <phase>package</phase>
                <configuration>
                  <message><![CDATA[${line.separator}
                =======================================================================================
                                        _
                 _ __     __ _    ___  | | __   __ _    __ _    ___
                | '_ \   / _` |  / __| | |/ /  / _` |  / _` |  / _ \
                | |_) | | (_| | | (__  |   <  | (_| | | (_| | |  __/
                | .__/   \__,_|  \___| |_|\_\  \__,_|  \__, |  \___|
                |_|                                    |___/
                =======================================================================================]]></message>
                </configuration>
              </execution>
              <execution>
                <id>show-message_verify</id>
                <goals>
                  <goal>echo</goal>
                </goals>
                <phase>verify</phase>
                <configuration>
                  <message><![CDATA[${line.separator}
                =======================================================================================
                                       _    __
                __   __   ___   _ __  (_)  / _|  _   _
                \ \ / /  / _ \ | '__| | | | |_  | | | |
                 \ V /  |  __/ | |    | | |  _| | |_| |
                  \_/    \___| |_|    |_| |_|    \__, |
                                                 |___/
                =======================================================================================]]></message>
                </configuration>
              </execution>
              <execution>
                <id>show-message_install</id>
                <goals>
                  <goal>echo</goal>
                </goals>
                <phase>install</phase>
                <configuration>
                  <message><![CDATA[${line.separator}
              skip-mvn-debug=${skip-mvn-debug}
              skip-mvn_debug=${skip-mvn_debug}
                =======================================================================================
                 _                 _             _   _
                (_)  _ __    ___  | |_    __ _  | | | |
                | | | '_ \  / __| | __|  / _` | | | | |
                | | | | | | \__ \ | |_  | (_| | | | | |
                |_| |_| |_| |___/  \__|  \__,_| |_| |_|
                =======================================================================================]]></message>
                </configuration>
              </execution>
              <execution>
                <id>show-message_site</id>
                <goals>
                  <goal>echo</goal>
                </goals>
                <phase>clean</phase>
                <configuration>
                  <message><![CDATA[${line.separator}
                =======================================================================================
                       _   _
                 ___  (_) | |_    ___
                / __| | | | __|  / _ \
                \__ \ | | | |_  |  __/
                |___/ |_|  \__|  \___|
                =======================================================================================]]></message>
                </configuration>
              </execution>
              <execution>
                <id>show-message_deploy</id>
                <goals>
                  <goal>echo</goal>
                </goals>
                <phase>deploy</phase>
                <configuration>
                  <message><![CDATA[${line.separator}
                =======================================================================================
                     _                  _
                  __| |   ___   _ __   | |   ___    _   _
                 / _` |  / _ \ | '_ \  | |  / _ \  | | | |
                | (_| | |  __/ | |_) | | | | (_) | | |_| |
                 \__,_|  \___| | .__/  |_|  \___/   \__, |
                               |_|                  |___/
                =======================================================================================]]></message>
                </configuration>
              </execution>
            </executions>
          </plugin>

          <!-- display active profile in validate phase -->
          <plugin>
            <groupId>org.apache.maven.plugins</groupId>
            <artifactId>maven-help-plugin</artifactId>
            <version>${maven-help-plugin.version}</version>
            <executions>
              <execution>
                <id>show-profiles</id>
                <goals>
                  <goal>active-profiles</goal>
                </goals>
                <phase>validate</phase>
              </execution>
            </executions>
          </plugin>
        </plugins>
      </build>
    </profile>
    <profile>
      <id>POM-sort</id>
      <!-- Deaktiver profilen med -P POM-sort eller i settings.xml <activeProfile>POM-sort</activeProfile>-->
      <activation>
        <activeByDefault>false</activeByDefault>
      </activation>
      <properties>
        <profile-in-use_POM-sort>Yes (profile used)</profile-in-use_POM-sort>
      </properties>
      <build>
        <plugins>
          <plugin>
            <groupId>com.github.ekryd.sortpom</groupId>
            <artifactId>sortpom-maven-plugin</artifactId>
            <version>${mvn-sortpom-maven-plugin.version}</version>
            <configuration>
              <encoding>${project.encoding}</encoding>
              <expandEmptyElements>false</expandEmptyElements>
              <indentBlankLines>true</indentBlankLines>
              <keepBlankLines>true</keepBlankLines>
              <lineSeparator>\n</lineSeparator>
              <predefinedSortOrder>${sortpom-predefinedSortOrder}</predefinedSortOrder>
              <sortDependencies>artifactId, groupId</sortDependencies>
              <sortProperties>true</sortProperties>
            </configuration>
            <executions>
              <execution>
                <goals>
                  <goal>sort</goal>
                </goals>
                <phase>verify</phase>
              </execution>
            </executions>
          </plugin>
        </plugins>
      </build>
    </profile>
    <profile>
      <id>show-properties</id>
      <activation>
        <activeByDefault>false</activeByDefault>
        <property>
          <name>!skip-show-properties</name>
        </property>
      </activation>
      <properties>
