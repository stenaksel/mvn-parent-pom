    </profile>
    <profile>
      <id>POM-sort</id>
      <!-- Deaktiver profilen med -P POM-sort eller i settings.xml <activeProfile>POM-sort</activeProfile>-->
      <activation>
        <activeByDefault>false</activeByDefault>
      </activation>
      <properties>
        <profile-in-use_POM-sort>Yes (profile used)</profile-in-use_POM-sort>
      </properties>
      <build>
        <plugins>
          <plugin>
            <groupId>com.github.ekryd.sortpom</groupId>
            <artifactId>sortpom-maven-plugin</artifactId>
            <version>${mvn-sortpom-maven-plugin.version}</version>
            <configuration>
              <encoding>${project.encoding}</encoding>
              <expandEmptyElements>false</expandEmptyElements>
              <indentBlankLines>true</indentBlankLines>
              <keepBlankLines>true</keepBlankLines>
              <lineSeparator>\n</lineSeparator>
              <predefinedSortOrder>${sortpom-predefinedSortOrder}</predefinedSortOrder>
              <sortDependencies>artifactId, groupId</sortDependencies>
              <sortProperties>true</sortProperties>
            </configuration>
            <executions>
              <execution>
                <goals>
                  <goal>sort</goal>
                </goals>
                <phase>verify</phase>
              </execution>
            </executions>
          </plugin>
        </plugins>
      </build>
    </profile>
    <profile>
      <id>show-properties</id>
      <activation>
        <activeByDefault>false</activeByDefault>
        <property>
          <name>!skip-show-properties</name>
          <value>true</value>
        </property>
      </activation>
      <properties>
        <profile-in-use_show-properties>Yes (skip-show-properties=${skip-show-properties})</profile-in-use_show-properties>
        <skip-show-properties>false (Overridden by activated profile: "show-properties")</skip-show-properties>
      </properties>
      <build>
        <plugins>
          <plugin>
            <groupId>com.github.ekryd.echo-maven-plugin</groupId>
            <artifactId>echo-maven-plugin</artifactId>
            <version>${mvn-echo-maven-plugin.version}</version>
            <inherited>true</inherited>
            <executions>
              <execution>
                <id>mvn-parent-pom show-properties</id>
                <goals>
                  <goal>echo</goal>
                </goals>
                <phase>validate</phase>
                <!--                <phase>${mvn-debug-default-phase}</phase>-->
                <configuration>
                  <!--suppress UnresolvedMavenProperty -->
                  <!-- // @formatter:off -->
                  <message>${line.separator}
      show-properties ("${project.name}")
      skip-mvn-debug=${skip-mvn-debug}, mvn-debug-reduced=${mvn-debug-reduced}
      java.runtime.version=${java.runtime.version}
      =======================================================================================
      Maven_kjørte "${project.build.finalName}", og profilen show-properties ble benyttet:
      (Maven (v ${maven.version}) fra ${maven.home}
      ${profile-mvn-debug-info}
      ---------------------------------------------------------------------------------------
      mvn-parent-pom reporting: ${profile-mvn-debug-info}
      =======================================================================================</message>
                  <!-- // @formatter:on -->
                </configuration>
              </execution>
            </executions>
          </plugin>
          <plugin>
            <groupId>org.apache.maven.plugins</groupId>
            <artifactId>maven-antrun-plugin</artifactId>
            <executions>
              <execution>
                <id>mvn-parent-pom show-properties antrun</id>
                <goals>
                  <goal>run</goal>
                </goals>
                <!--                <phase>${mvn-debug-default-phase}</phase>-->
                <phase>validate</phase>
                <configuration>
                  <target name="show-properties2" unless="mvn-debug-reduced">
                    <!--                    <Available />-->
                    <echoproperties />
                    <!--                    <echoproperties prefix="skip"/>-->
                  </target>
                </configuration>
              </execution>
            </executions>
          </plugin>
        </plugins>
      </build>
    </profile>
    <profile>
      <id>show-profiles-used</id>
      <activation>
        <activeByDefault>false</activeByDefault>
      </activation>
      <properties>
        <profile-in-use_-info>( Undefined properies will be shown between "${" and "}" )</profile-in-use_-info>
        <profile-in-use_show-profiles-used>Yes</profile-in-use_show-profiles-used>
      </properties>
      <build>
        <plugins>
          <!-- display active profile in validate phase -->
          <plugin>
            <groupId>org.apache.maven.plugins</groupId>
            <artifactId>maven-help-plugin</artifactId>
            <executions>
              <execution>
                <id>mvn-parent-pom mvn-debug show-profiles-used</id>
                <goals>
                  <goal>active-profiles</goal>
                </goals>
                <!--                <phase>${mvn-debug-default-phase}</phase>-->
                <phase>validate</phase>
                <!--                <phase>none</phase>-->
              </execution>
            </executions>
          </plugin>
          <plugin>
            <groupId>org.apache.maven.plugins</groupId>
            <artifactId>maven-antrun-plugin</artifactId>
            <executions>
              <execution>
                <goals>
                  <goal>run</goal>
                </goals>
                <phase>validate</phase>
                <configuration>
                  <target name="default-show-profiles-used">
                    <echoproperties prefix="line.-_-_" />
                    <echoproperties prefix="line.----" />
                    <echoproperties prefix="profile-in-use_" />
                    <echoproperties prefix="line.-_-_" />
                    <echoproperties prefix="line.----" />
                  </target>
                </configuration>
              </execution>
            </executions>
          </plugin>
        </plugins>
      </build>
    </profile>
    <profile>
      <id>show-version-info</id>
      <activation>
        <activeByDefault>false</activeByDefault>
        <!-- Deactivate profile using -Dskip-show-version-info or -P !show-version-info -->
        <property>
          <name>!skip-show-version-info</name>
          <!--          <value>true</value>-->
        </property>
      </activation>
      <properties>
        <profile-in-use_show-version-info>Yes (show-version-info=${show-version-info},
          skip?-=${skip-show-version-info})</profile-in-use_show-version-info>
        <skip-show-version-info>false (Overridden by activated profile: "show-version-info")</skip-show-version-info>
      </properties>
      <build>
        <plugins>
          <plugin>
            <groupId>org.codehaus.mojo</groupId>
            <artifactId>versions-maven-plugin</artifactId>
            <version>${mvn-versions-maven-plugin.version}</version>
            <executions>
              <execution>
                <goals>
                  <goal>display-dependency-updates</goal>
                  <goal>display-plugin-updates</goal>
                </goals>
                <phase>compile</phase>
              </execution>
            </executions>
          </plugin>
        </plugins>
        
      </build>
    </profile>
    <profile>
      <id>mvn-debug-reduced</id>
      <!--
      If you add the mvn-debug to your .m2/settings.xml file as one of your default active profiles, like this:
      <activeProfiles>
        <activeProfile>mvn-debug</activeProfile>
      </activeProfiles>
      You can use the "mvn-debug-reduced" profile to reduce some of the "default" features from the mvn-parent-pom.
      Some profiles will kick in by default when the "mvn-debug" profile is used, but this "default" can be reduced by
      using "mvn-debug-reduced".
      For example, the "show-profiles-used" profile will also be activated by default when "mvn-debug" profile is used,
      but not activated by default using "mvn-debug-reduced".
      If you force the use of some profiles activatged by default "mvn-debug", the "mvn-debug-reduced" will have no effect.
      (You may off-cource add the "mvn-debug-reduced" to your active profiles settings if you like)
       -->
      <activation>
        <activeByDefault>false</activeByDefault>
        <property>
          <name>!mvn-debug-reduced</name>
          <value>true</value>
        </property>
      </activation>
      <properties>
        <mvn-debug-reduced>true</mvn-debug-reduced>
        <profile-in-use_mvn-debug-reduced>Yes</profile-in-use_mvn-debug-reduced>
        <profile-mvn-debug-info>Using "mvn-debug-reduced" profile!</profile-mvn-debug-info>
        <!-- Reducing default profiles -->
        <!--
                <skip-show-profiles-used>true</skip-show-profiles-used>
                <mvn-debug-default-phase>none</mvn-debug-default-phase>
        -->
      </properties>
      <build>
        <plugins>
          <plugin>
            <groupId>com.github.ekryd.echo-maven-plugin</groupId>
            <artifactId>echo-maven-plugin</artifactId>
            <version>${mvn-echo-maven-plugin.version}</version>
            <inherited>true</inherited>
            <executions>
              <execution>
                <id>mvn-parent-pom mvn-debug-reduced</id>
                <goals>
                  <goal>echo</goal>
                </goals>
                <!--            <phase>install</phase>-->
                <configuration>
                  <!--suppress UnresolvedMavenProperty -->
                  <!-- // @formatter:off -->
                  <message>${line.separator}
      "${project.name}":
      =======================================================================================
      mvn-debug-reduced
      Maven_kjørte "${project.build.finalName}", og profilen mvn-debug-reduced ble benyttet:
      (Maven (v ${maven.version}) fra ${maven.home}
      ${profile-mvn-debug-info}
      ---------------------------------------------------------------------------------------
      mvn-parent-pom reporting: ${profile-mvn-debug-info}
      =======================================================================================
      | Det er mvn-parent-pom (${project.version}) som har konfigurert deler
      | av dette bygget, som er kjørt av ${maven.home}
      | Maven profilen mvn-debug-reduced er benyttet for å vise denne informasjonen
      =======================================================================================</message>
                  <!-- // @formatter:on -->
                </configuration>
              </execution>
            </executions>
          </plugin>
        </plugins>
      </build>
    </profile>
    
  </profiles>
  
</project>
