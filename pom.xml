                </requireMavenVersion>
              </rules>
            </configuration>
          </execution>
          <execution>
            <id>mvn-parent-pom maven-enforcer-plugin requireJavaVersion</id>
            <goals>
              <goal>enforce</goal>
            </goals>
            <configuration>
              <rules>
                <requireJavaVersion>
                  <version>[${mvn-parent-require.min-version.java},]</version>
                  <!-- // @formatter:off -->
                  <message>${line.separator}
      ${i18n.maven.run.info} [sun.java.command]:
