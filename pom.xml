<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>

  <groupId>ske.felles</groupId>
  <artifactId>mvn-parent-pom</artifactId>
  <version>1.0-SNAPSHOT</version>
  <packaging>pom</packaging>
  <name>mvn-parent-pom</name>
  <!--
  Activate profiles by adding them to your in your ~/.m2/settings.xml file, like shown below:
	<activeProfiles>
		<activeProfile>mvn-debug</activeProfile>
		<activeProfile>show-profiles-used</activeProfile>
		<activeProfile>show-version-info</activeProfile>
	</activeProfiles>
  Activate the "mvn-debug" profile to get hints for the use and configuration of your project.
  Activate the "show-version-info" to be informed of new versions of dependencies and plugins.
  -->
  <properties>
    <?SORTPOM IGNORE?>
    <!--    <show-version-info>true</show-version-info>-->
    <!-- Line above because we use the sortpom-maven-plugin (to keep the file sorted), but not here (until RESUME) -->
    <project.encoding>UTF-8</project.encoding>
    <project.build.sourceEncoding>${project.encoding}</project.build.sourceEncoding>
    <project.reporting.outputEncoding>${project.encoding}</project.reporting.outputEncoding>
    <sortpom-predefinedSortOrder>custom_1</sortpom-predefinedSortOrder>
    <?SORTPOM RESUME?>

    <aurora-require.compileSource>1.8</aurora-require.compileSource>
    <aurora-require.min-version.java>1.8.0-191</aurora-require.min-version.java>
    <aurora-require.min-version.maven>3.6.1</aurora-require.min-version.maven>
    <enforce-version-consistency.version>0.0.13</enforce-version-consistency.version>
    <maven-antrun-plugin.version>3.0.0</maven-antrun-plugin.version>
    <maven-clean-plugin.version>3.1.0</maven-clean-plugin.version>
    <maven-deploy-plugin.version>3.0.0-M1</maven-deploy-plugin.version>
    <maven-enforcer-plugin.version>3.0.0-M3</maven-enforcer-plugin.version>
    <maven-failsafe-plugin.version>3.0.0-M5</maven-failsafe-plugin.version>
    <maven-help-plugin.version>3.2.0</maven-help-plugin.version>
    <maven-install-plugin.version>3.0.0-M1</maven-install-plugin.version>
    <maven-project-info-reports-plugin.version>3.1.0</maven-project-info-reports-plugin.version>
    <maven-reactor-plugin.version>1.1</maven-reactor-plugin.version>
    <maven-site-plugin.version>3.9.1</maven-site-plugin.version>
    <mvn-debug>true</mvn-debug>
    <mvn-echo-maven-plugin.version>1.3.0</mvn-echo-maven-plugin.version>
    <mvn-nexus-staging-maven-plugin.version>1.6.8</mvn-nexus-staging-maven-plugin.version>
    <mvn-sortpom-maven-plugin.version>2.11.0</mvn-sortpom-maven-plugin.version>
    <mvn-spotbugs-maven-plugin.version>4.0.4</mvn-spotbugs-maven-plugin.version>
    <mvn-versions-maven-plugin.version>2.7</mvn-versions-maven-plugin.version>
    <profile-in-use-info>Profile mvn-debug was not used!</profile-in-use-info>
    <!--    <skip-show-properties>${skip-mvn-debug}</skip-show-properties>-->
  </properties>

  <build>

    <pluginManagement>
      <plugins>
        <plugin>
          <groupId>org.apache.maven.plugins</groupId>
          <artifactId>maven-site-plugin</artifactId>
          <version>${maven-site-plugin.version}</version>
        </plugin>
        <plugin>
          <groupId>org.apache.maven.plugins</groupId>
          <artifactId>maven-clean-plugin</artifactId>
          <version>${maven-clean-plugin.version}</version>
        </plugin>
        <plugin>
          <groupId>org.apache.maven.plugins</groupId>
          <artifactId>maven-deploy-plugin</artifactId>
          <version>${maven-deploy-plugin.version}</version>
        </plugin>
        <plugin>
          <groupId>org.apache.maven.plugins</groupId>
          <artifactId>maven-install-plugin</artifactId>
          <version>${maven-install-plugin.version}</version>
        </plugin>
        <plugin>
          <groupId>org.apache.maven.plugins</groupId>
          <artifactId>maven-project-info-reports-plugin</artifactId>
          <version>${maven-project-info-reports-plugin.version}</version>
        </plugin>
        <plugin>
          <groupId>org.apache.maven.plugins</groupId>
          <artifactId>maven-reactor-plugin</artifactId>
          <version>${maven-reactor-plugin.version}</version>
        </plugin>
      </plugins>
    </pluginManagement>

    <plugins>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-enforcer-plugin</artifactId>
        <version>${maven-enforcer-plugin.version}</version>
        <dependencies>
          <dependency>
            <groupId>ske.prosess.styring.saksmappe.enforce-version-consistency</groupId>
            <artifactId>enforce-version-consistency</artifactId>
            <version>${enforce-version-consistency.version}</version>
          </dependency>
        </dependencies>
        <executions>
          <execution>
            <id>requireMavenVersion mvn-parent-pom</id>
            <goals>
              <goal>enforce</goal>
            </goals>
            <configuration>
              <rules>
                <requireMavenVersion>
                  <!-- // @formatter:off -->
                  <!--suppress UnresolvedMavenProperty -->
                  <message>(Maven kjørte "${project.name}":

                      /\/\   __ ___   _____ _ __
                     /    \ / _` \ \ / / _ \ '_ \
                    / /\/\ \ (_| |\ V /  __/ | | |
                    \/    \/\__,_| \_/ \___|_| |_| ${maven.version}
                    fra ${maven.home}

                    For gammel versjon av Maven benyttes!
                    (I bruk nå er versjon = ${maven.version} fra ${maven.home})
                    Krav konfigurert mvn-parent-pom ('requireMavenVersion', krever versjon &gt;=
                    ${mvn.minVer})
                    (Kravet kan overstyres ved å justere property "aurora-require.min-version.maven". Ikke anbefalt!)</message>
                  <!-- // @formatter:on -->
                  <version>[${aurora-require.min-version.maven},)</version>
                </requireMavenVersion>
              </rules>
            </configuration>
          </execution>
          <execution>
            <id>requireJavaVersion mvn-parent-pom</id>
            <goals>
              <goal>enforce</goal>
            </goals>
            <configuration>
              <rules>
                <requireJavaVersion>
                  <version>[${aurora-require.min-version.java},]</version>
                  <message>Maven kjørte "${project.name}" med følgende parametere [sun.java.command]:
                    ${sun.java.command}
                    ==========================================================
                    For gammel versjon av Java benyttes (v. ${java.version} )!
                    ==========================================================
                    Krav konfigurert i mvn-parent-pom ('requireJavaVersion')
                    krever versjon &gt;= ${aurora-require.min-version.java}!
                    Nå var det versjon ${java.version} som ble benyttet - fra ${java.library.path})

                    (Kravet kan overstyres ved å justere property "aurora-require.min-version.java". Ikke anbefalt!)
                  </message>
                </requireJavaVersion>
              </rules>
            </configuration>
          </execution>
          <execution>
            <id>requirePluginVersions mvn-parent-pom</id>
            <goals>
              <goal>enforce</goal>
            </goals>
            <configuration>
              <rules>
                <requirePluginVersions>
                  <message>Maven kjørte "${project.name}" med følgende parametere [sun.java.command]:
                    ${sun.java.command}
                    ==============================================================
                      Alle plugins som benyttes må ha en spesifisert versjon!
                    ==============================================================
                    Krav konfigurert i mvn-parent-pom ('requirePluginVersions')
                  </message>
                  <banLatest>true</banLatest>
                  <banRelease>true</banRelease>
                  <banSnapshots>true</banSnapshots>
                  <phases>clean,deploy,site</phases>
                  <additionalPlugins>
                    <additionalPlugin>org.apache.maven.plugins:maven-reactor-plugin</additionalPlugin>
                  </additionalPlugins>
                  <unCheckedPluginList>org.apache.maven.plugins:maven-enforcer-plugin</unCheckedPluginList>
                </requirePluginVersions>
              </rules>
            </configuration>
          </execution>
          <execution>
            <id>dependencyConvergence mvn-parent-pom</id>
            <goals>
              <goal>enforce</goal>
            </goals>
            <configuration>
              <rules>
                <dependencyConvergence>
                  <uniqueVersions>true</uniqueVersions>
                </dependencyConvergence>
              </rules>
            </configuration>
          </execution>
          <!--
          <execution>
            <id>enforce4aurora-parent</id>
