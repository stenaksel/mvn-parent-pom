        <skip-show-properties>false (Overridden by profile activation: "show-properties")</skip-show-properties>
      </properties>
      <build>
        <plugins>
          <plugin>
            <groupId>com.github.ekryd.echo-maven-plugin</groupId>
            <artifactId>echo-maven-plugin</artifactId>
            <version>${mvn-echo-maven-plugin.version}</version>
            <inherited>true</inherited>
            <executions>
              <execution>
                <id>show-message4show-properties</id>
                <goals>
                  <goal>echo</goal>
                </goals>
                <phase>validate</phase>
                <configuration>
                  <!--suppress UnresolvedMavenProperty -->
                  <message>${line.separator}
                    skip-mvn-debug=${skip-mvn-debug}
                    skip-mvn_debug=${skip-mvn_debug}
                    =======================================================================================
                    show-properties
                    profilen "show-properties" ble benyttet:
                    Maven_kjørte "${project.build.finalName}", og profilen skip-mvn-debug ble benyttet:
                    (Maven (v ${maven.version}) fra ${maven.home}
                    ${profile-in-use-info}
                    ---------------------------------------------------------------------------------------
                    mvn-parent-pom reporting: ${profile-in-use-info}
                    =======================================================================================
                  </message>
                </configuration>
              </execution>
            </executions>
          </plugin>
          <plugin>
            <groupId>org.apache.maven.plugins</groupId>
            <artifactId>maven-antrun-plugin</artifactId>
            <version>${maven-antrun-plugin.version}</version>
            <executions>
              <execution>
                <goals>
                  <goal>run</goal>
                </goals>
                <phase>validate</phase>
                <configuration>
                  <target>
                    <echoproperties />
                    <!--                    <echoproperties prefix="skip"/>-->
                  </target>
                </configuration>
              </execution>
            </executions>
          </plugin>
        </plugins>
      </build>
    </profile>
